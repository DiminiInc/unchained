{
  "manifest_version": 2,

  "name": "Unchained",
  "version": "0.0.1",
  "description": "Websites blocking",

  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "browser_action": {
      "default_icon": "shackles.png",
      "default_popup": "uh-oh.html"
    }
}